#= require egg
#= require mustache

class @<%= app_name.camelize %> extends egg.View

  @init (opts)->
    @server = egg.RestApi.create(headers: {'X-CSRF-Token': opts.csrfToken})
    @localStore = egg.Datastore.create(store: localStorage)
    
    # Kick off your app here!
    alert("Welcome to my awesome <%= app_name.camelize %>!")
