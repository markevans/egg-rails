define ['egg', 'jst!<%= underscore_name %>'], (egg, jst) ->

  class <%= view_name.camelize %> extends egg.View

    @className: '<%= underscore_name %>'

    # @listen 'model:event', 'method'

    # @onDOM '.selector', 'event', 'delegateMethod'

    @init (opts)->
      @render()

    render: ->
      $(@elem).html jst(@presenter().toJSON())
